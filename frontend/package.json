{
  "name": "apollo-frontend",
  "type": "module",
  "description": "Astro frontend for the ApostropheCMS + Astro Apollo template",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env APOS_EXTERNAL_FRONT_KEY=dev astro dev",
    "start": "node ./dist/server/entry.mjs",
    "build": "astro build",
    "serve": "cross-env HOST=0.0.0.0 node ./dist/server/entry.mjs",
    "preview": "cross-env DEBUG=* astro preview",
    "astro": "astro"
  },
  "engines": {
    "node": "^18.17.1 || ^20.3.0 || >=22.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@apostrophecms/apostrophe-astro": "^1.7.1",
    "@astrojs/node": "^9.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "accordion-js": "^3.3.4",
    "astro": "^5.0.9",
    "bulma": "^1.0.2",
    "dayjs": "^1.11.10",
    "postcss-viewport-to-container-toggle": "^1.1.0",
    "vite": "^5.0.7"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "sass-embedded": "^1.81.0"
